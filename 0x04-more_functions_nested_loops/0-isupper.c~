#include<stdio.h>
#include "main.h"

/**
 *main- a function that checks for uppercase character
 *return 1 if c is uppercase
 *return 0 if otherwise
 */

int main(void)
{
  int c;  
  int _isupper(int c)
  {
    if (c >= 'A' && c <= 'Z')
      return (1);
  }
  else
    {
      return (0);
    }
  int _supper(int c);
}
